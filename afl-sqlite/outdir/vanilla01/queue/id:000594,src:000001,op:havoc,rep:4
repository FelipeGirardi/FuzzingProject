CREATE TABLE t1(a,b);
CREATE INDEX t0a ON t1(a);
CREATE INDEX t0b ON t1(b);
INSERT INTO t1 VALUES(1,NULL);
INSERT INTO t1 SELECT a+1, b FROM t1;
INSERT INTO t1 SELECT a+2, b FROM t1;
INSERT INTO t1 SELECT a+4, b FROM t1;
INSERT INTO t1 SELECT a+ 'a''b'OM t1;
INSERT INTO t1 SELECT a+16,b F0OM t1;
INSERT INTO t1 SELECT a+32,b FROV t1;
INSERT INTO t1 SELECT a+64,NSERT INTO t1 VALUES(1,NULL);b FROM t1;
ANALYZE;
