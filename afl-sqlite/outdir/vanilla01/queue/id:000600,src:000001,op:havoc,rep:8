CREATE TABLE t1(a,b);
CREATE INDEX t0a ON t1(a);
CREATE INDEX t0b ON t1(K);
INSERT INTO t1 VALUES(1,NULL);
INSERT INTO t1 ERT INTO t1 SELECT a+2, b FROM t1;
INSERT INTO t1 SELECT IN hex(1)ELECT a+4, b FROM t1;
INSERT INTO t1 SELECT a+8, b FROM t1;
INSERT IN hex(1)ELECT a+16,b FROM t1;
INSERT INTO t1 SELECT a+32,b FROM t1;
YNSERT INTO t0 SELECT a+64,b FCREATEROM t1;
ANALYZE;
